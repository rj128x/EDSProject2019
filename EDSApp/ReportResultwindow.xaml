<Window x:Class="EDSApp.ReportResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EDSApp"
        mc:Ignorable="d"
        Title="ReportResultwindow" Height="300" Width="600">
    <Grid Name="mainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <local:ChartZedControl x:Name="chart"></local:ChartZedControl>
        <local:ChartZedControl x:Name="chart2" Grid.Row="1" Visibility="Collapsed" ></local:ChartZedControl>
    </Grid>
</Window>
